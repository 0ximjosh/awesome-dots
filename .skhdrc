lalt - return : open -n /Applications/Ghostty.App
lalt - b : firefox
lalt - l : /Applications/Linear.app/Contents/MacOS/linear

ctrl + lalt - w : /Users/josh/skhd/wallpaper.sh

lalt + shift - j : yabai -m window --focus prev
lalt + shift - k : yabai -m window --focus next
lalt + shift - h : ~/skhd/cycle_clockwise.sh
lalt + shift - l : ~/skhd/cycle_counterclockwise.sh

lalt - f : yabai -m window --toggle topmost
lalt - m : yabai -m window --toggle native-fullscreen
lalt - c : yabai -m window --toggle float

cmd - k : yabai -m window --space next --focus
cmd - j : yabai -m window --space prev --focus
cmd + shift - k : yabai -m space --focus next
cmd + shift - j : yabai -m space --focus prev

cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5
cmd - 6 : yabai -m space --focus 6
cmd - 7 : yabai -m space --focus 7
cmd - 8 : yabai -m space --focus 8
cmd - 9 : yabai -m space --focus 9

cmd + ctrl - 1 : yabai -m window --space 1 --focus 
cmd + ctrl - 2 : yabai -m window --space 2 --focus 
cmd + ctrl - 3 : yabai -m window --space 3 --focus 
cmd + ctrl - 4 : yabai -m window --space 4 --focus 
cmd + ctrl - 5 : yabai -m window --space 5 --focus 
cmd + ctrl - 6 : yabai -m window --space 6 --focus 
cmd + ctrl - 7 : yabai -m window --space 7 --focus 
cmd + ctrl - 8 : yabai -m window --space 8 --focus 
cmd + ctrl - 9 : yabai -m window --space 9 --focus 

lalt + shift - p : osascript -e 'tell app "System Events" to shut down'
